{
  "name": "equipment-tracker-fullstack",
  "version": "1.0.0",
  "description": "Full-stack Equipment Tracker application with React frontend and Express backend",
  "scripts": {
    "install:all": "npm run install:backend && npm run install:frontend",
    "install:backend": "cd backend/backend && npm install",
    "install:frontend": "cd frontend/equipment-hub-main && npm install",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend/backend && npm run dev",
    "dev:frontend": "cd frontend/equipment-hub-main && npm run dev",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd backend/backend && npm start",
    "start:frontend": "cd frontend/equipment-hub-main && npm run preview"
  },
  "keywords": [
    "equipment",
    "tracker",
    "fullstack",
    "react",
    "express",
    "sqlite"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.2.1"
  }
}
